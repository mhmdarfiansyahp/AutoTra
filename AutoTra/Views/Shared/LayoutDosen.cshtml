<body>
    <header>
        <div class="polman-nav-static-top">
            <div class="float-left">
                <a href="#">
                    <img src="~/Content/Images/IMG_1687.png" style="height: 50px;" />
                </a>
            </div>
            <div class="polman-menu">
                <div style="padding-top: 15px; margin-right: -30px;">
                    Hai, <span style="font-size: 20px; font-weight: bold;"> @Context.Session.GetString("nama")</span>
                </div>
            </div>


            <div class="polman-menu-bar">
                <div class="float-right">
                    <div class="fa fa-bars fa-2x" style="margin-top: 9px; cursor: pointer;" aria-hidden="true" data-toggle="collapse" data-target="#menu" aria-expanded="false" aria-controls="menu"></div>
                </div>
            </div>

        </div>
    </header>


    <div class="polman-nav-static-right collapse scrollstyle" id="menu">
        <div id="accordions" role="tablist" aria-multiselectable="true">
            <div class="list-group">
                <a class="list-group-item list-group-item-action polman-username" style="border-radius: 0px; border: none; background-color: #EEE;">Hai,&nbsp;</a>
                    <a href='/Pages/Admin/Dashboard.aspx' class='list-group-item list-group-item-action <%: Page.Title == "Dashboard Admin" ? "polman-menu-active" : "" %>' style='border-radius: 0px; border: none; padding-left: 22px; display: inherit;'>
                    <i class='fa fa-home fa-lg fa-pull-left'></i>Dashboard
                </a>

                <a href='#menu1' role='tab' class='list-group-item list-group-item-action' data-toggle='collapse' data-parent='#accordion' aria-expanded='false' aria-controls='menu1' style='border-radius: 0px; border: none;'>
                    <i class='fa fa-chevron-down fa-lg fa-pull-left'></i>User Data
                </a>
                <div id='menu1' class='collapse' role='tabpanel'>
                    <a asp-controller="PIC" asp-action="Index" class='list-group-item list-group-item-action' style='border-radius: 0px; border: none; padding-left: 47px; display: flex; align-items: center;'>
                        <i class='fa fa-user fa-lg fa-pull-left' style="margin-top: 2px;"></i>PIC Data

                    </a>
                </div>

                <a asp-controller="Mobil" asp-action="Index" class='list-group-item list-group-item-action <%: Page.Title == "Laporan Kuesioner Excel" ? "polman-menu-active" : "" %>' style='border-radius: 0px; border: none; padding-left: 22px; display: inherit;'>
                    <i class='fa fa-solid fa-car fa-lg fa-pull-left' style="margin-top: 2px;"></i>Vehicle Data
                </a>

                <a href='#menu2' role='tab' class='list-group-item list-group-item-action' data-toggle='collapse' data-parent='#accordions' aria-expanded='false' aria-controls='menu2' style='border-radius: 0px; border: none; display: flex; align-items: center;'>
                    <i class='fa fa-chevron-down fa-lg' style="margin-right: 10px;"></i>Form Data
                </a>

                <div id='menu2' class='collapse' role='tabpanel'>
                    <a href='' class='list-group-item list-group-item-action' style='border-radius: 0px; border: none; padding-left: 47px; display: flex; align-items: center; flex-wrap: wrap;'>
                        <i class='fa fa-solid fa-file-text fa-lg fa-pull-left'></i>Inspection Standard <br>
                        Data
                    </a>

                    <a href='' class='list-group-item list-group-item-action' style='border-radius: 0px; border: none; padding-left: 47px; display: flex; align-items: center; flex-wrap: wrap;'>
                        <i class='fa fa-solid fa-file-text fa-lg fa-pull-left'></i>Inspection Category <br>
                        Data
                    </a>


                    <a href='' class='list-group-item list-group-item-action' style='border-radius: 0px; border: none; padding-left: 47px; display: flex; align-items: center;'>
                        <i class='fa fa-solid fa-file-text fa-lg fa-pull-left'></i>Inspection Item Data
                    </a>

                    <a href='' class='list-group-item list-group-item-action' style='border-radius: 0px; border: none; padding-left: 47px; display: flex; align-items: center;'>
                        <i class='fa fa-solid fa-file-text fa-lg fa-pull-left'></i>Inspection Form Data
                    </a>
                </div>

@*                <a href='/Pages/Admin/Laporan_Kuesioner_Excel.aspx' class='list-group-item list-group-item-action <%: Page.Title == "Laporan Kuesioner Excel" ? "polman-menu-active" : "" %>' style='border-radius: 0px; border: none; padding-left: 22px; display: inherit;'>
                    <i class='fa fa-file-excel-o fa-lg fa-pull-left'></i>Laporan Kuesioner Excel
                </a>

                <a href='/Pages/Admin/Pengingat.aspx' class='list-group-item list-group-item-action <%: Page.Title == "Pengingat" ? "polman-menu-active" : "" %>' style='border-radius: 0px; border: none; padding-left: 22px; display: inherit;'>
                    <i class='fa fa-bell fa-lg fa-pull-left'></i>Pengingat
                </a>

                <a href='/Pages/Admin/Konfirmasi_Akun.aspx' class='list-group-item list-group-item-action <%: Page.Title == "Konfirmasi Akun" ? "polman-menu-active" : "" %>' style='border-radius: 0px; border: none; padding-left: 22px; display: inherit;'>
                    <i class='fa fa-check-circle fa-lg fa-pull-left'></i>Konfirmasi Akun
                </a>

                <a href="https://sia.polman.astra.ac.id/sso" class="list-group-item list-group-item-action" style="border-radius: 0px; border: none; padding-left: 23px;"><i class='fa fa-external-link fa-lg fa-pull-left'></i>Halaman SSO</a>
*@            </div>

            <div class="polman-adjust5" style="display: flex; flex-direction: column; height: 100%;">
                <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server"></asp:ContentPlaceHolder>

                <div style="flex-grow: 1;"></div> <!-- Penambahan elemen dummy untuk mengisi ruang sisa -->

                <a asp-controller="Home" asp-action="Index" class="list-group-item list-group-item-action" style="border-radius: 0px; border: none; padding-left: 23px;">
                    <i class="fa fa-sign-out fa-lg fa-pull-left"></i>Logout
                </a>
            </div>

        </div>
    </div>

    <div class="polman-adjust5">
        <ol class="breadcrumb polman-breadcrumb">
            <li class="breadcrumb-item"><a href="https://sia.polman.astra.ac.id/sso" data-toggle="tooltip" data-placement="bottom" title="" data-original-title="Menuju Halaman SSO">AutoTra</a></li>
            <li class="breadcrumb-item">@ViewData["Title"]</li>
        </ol>

        <hr />

        <asp:ContentPlaceHolder ID="ContentPlaceHolder1" runat="server">
        </asp:ContentPlaceHolder>
    </div>

    @RenderBody()

    <asp:ContentPlaceHolder ID="ContentPlaceHolder2" runat="server">
    </asp:ContentPlaceHolder>

</body>
</html>
